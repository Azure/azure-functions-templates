    <Exec Command="for /R .\bin\Portal\temp\Templates\ %%x in (*.cs) do ren &quot;%%x&quot; *.csx" />
    <Exec Command="..\tools\nuget.exe pack .\bin\Portal\temp\PortalTemplates.nuspec -Version $(PortalTemplateVersion) -OutputDirectory .\bin\Portal\temp\nuget\" />
    <Exec Command="..\tools\nuget.exe install Azure.Functions.Templates.Portal -Version $(PortalTemplateVersion) -Source $(MSBuildProjectDirectory)\bin\Portal\temp\nuget -OutputDirectory .\bin\Portal\Release\ -ExcludeVersion" />
