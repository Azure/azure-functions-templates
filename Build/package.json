{
  "name": "server",
  "version": "0.0.0",
  "description": "Backend for Functions Portal",
  "author": "Microsoft",
  "license": "MIT",
  "private": true,
  "scripts": {
    "bundle": "gulp build-production",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:topdev": "npm run build:resources && nodemon",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/server.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build:resources": "gulp build-all"
  },
  "dependencies": {
    "@azure/storage-blob": "^10.3.0",
    "@azure/storage-file": "^10.1.0",
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^9.0.5",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/swagger": "^4.8.2",
    "applicationinsights": "^1.3.1",
    "async-file": "^2.0.2",
    "axios": "^1.6.0",
    "cache-manager": "^2.9.0",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "csurf": "^1.10.0",
    "dotenv": "^8.0.0",
    "express-rate-limit": "^3.5.0",
    "gulp-cli": "^2.2.0",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.1",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.12.2",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "@types/supertest": "^2.0.7",
    "del": "^4.1.1",
    "gulp": "^4.0.1",
    "gulp-decompress": "^2.0.2",
    "gulp-download-stream": "^0.0.19",
    "gulp-json-editor": "^2.5.2",
    "gulp-merge-json": "^2.1.2",
    "gulp-nuget": "^1.3.0",
    "gulp-prettier": "^2.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-token-replace": "^1.1.5",
    "gulp-wait": "0.0.2",
    "gulp-zip": "^5.0.1",
    "jest": "^26.6.1",
    "merge-stream": "^1.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^1.17.0",
    "request": "^2.88.2",
    "resx": "^2.0.4",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "through2": "^3.0.1",
    "ts-jest": "^26.4.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.10.1",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
